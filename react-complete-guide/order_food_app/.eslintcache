{classes.button} ${btnIsHighlighted ? classes.bump : ''}`;\n\n    useEffect(() => {\n        if (cartCtx.items.length === 0) {\n            return;\n        }\n\n        setBtnIsHighlighted(true);\n\n        const timer = setTimeout(() => {\n            setBtnIsHighlighted(false);\n        }, 300);\n\n        return () => {\n            clearTimeout(timer);\n        };\n    }, [items]);\n\n    return (\n        <button className={btnClasses} onClick={props.onClick}>\n            <span className={classes.icon}>\n                <CartIcon />\n            </span>\n            <span>Your Cart</span>\n            <span className={classes.badge}>\n                {nrOfCartItems}\n            </span>\n        </button>\n    );\n};\n\nexport default HeaderCartButton;","/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/Meals/MealsSummary.js",[],"/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/UI/Modal.js",[],"/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/Cart/CartIcon.js",[],"/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/UI/Card.js",[],"/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/Meals/MealItem/MealItem.js",[],"/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/Meals/MealItem/MealItemForm.js",[],"/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/UI/Input.js",[],"/Users/patrycjakierejewska/Documents/GitHub/react-course-udemy/react-complete-guide/order_food_app/src/components/Cart/Checkout.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":35,"column